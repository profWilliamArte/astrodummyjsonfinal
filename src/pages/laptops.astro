---
import Cartprod from '../components/Cartprod.astro';
import Layout from '../layout/Layout.astro';
const response = await fetch('https://dummyjson.com/products/category/laptops');
const data = await response.json();
---
<Layout>
    <div class="container">
        <h4 class="text-center py-4 titulo">Laptops ({ data.products.length}) </h4>
        <div class="row">
        { 
            data.products.map((item)=>(
                <Cartprod item={item} />
            ))
        }
    </div>
</Layout>
